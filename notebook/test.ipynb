    "from typing import List\n",
    "\n",
    "def find_start_of_sentence(input_texts: List[str], start_index: int) -> int:\n",
    "\tfor i in range(start_index, -1, -1):\n",
    "\t\tif input_texts[i][-1] in ['.', '?', '!', ':']:\n",
    "\t\t\treturn i + 1\n",
    "\treturn 0\n",
    "\n",
    "print(find_start_of_sentence([\"How\", \"are\", \"you?\"], 2))"
   ]
  }
